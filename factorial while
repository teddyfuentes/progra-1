// factorial.cpp : Defines the entry point for the console application.
//
#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{ 
	int i,n,p=1;
	do{
	cout<<"ingresar un numero: "<<endl;
	cin>>n;
	}while (n<0);
	i=1;
	while(i<=n)
	{
		p=p*i;
	    i++;
	}	
	cout<<"el factorial es: "<<p<<endl;
	getch();
}
