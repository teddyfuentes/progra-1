// factorial.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int n,i,p=1;
	do	{
	cout<<"ingresar un numero"zzendl;
		cin>>n;
	}while (n<0);
	
	for (i=1;i<=n;i++)
	
			p=p*i;
	  
	cout<<"el factorial es:"<<p<<endl;
	getch();
    
}

